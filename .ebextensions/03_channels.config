container_commands:
  01_copy_supervisord_conf:
    command: "cp .ebextensions/supervisord/supervisord.conf /opt/python/etc/supervisord.conf"
  02_copy_supervisord_conf:
    command: "cp .ebextensions/supervisord/supervisord.conf /usr/local/etc/supervisord.conf"
  03_reload_supervisod:
    command: "/usr/local/bin/supervisord -c /usr/local/etc/supervisord.conf reload"

  04_reload_supervisord:
    command: "supervisorctl -c /opt/python/etc/supervisord.conf reload"